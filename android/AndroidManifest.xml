<?xml version='1.0' encoding='utf-8'?>
<manifest package="org.kde.necessitas.Monarq" android:versionName="0.0.1" android:versionCode="1" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="10"/>
